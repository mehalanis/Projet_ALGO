<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="algo.ABRController">
   <top>
      <AnchorPane prefHeight="44.0" style="-fx-background-color: #ffffff;" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="text" layoutX="14.0" layoutY="10.0" prefHeight="30.0" prefWidth="200.0">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <Button layoutX="213.0" layoutY="10.0" mnemonicParsing="false" onAction="#insert" prefHeight="30.0" text="Insert">
               <font>
                  <Font size="16.0" />
               </font></Button>
            <Button layoutX="275.0" layoutY="10.0" mnemonicParsing="false" onAction="#Rechercher" prefHeight="30.0" text="Rechercher">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></AnchorPane>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" BorderPane.alignment="TOP_CENTER">
         <content>
            <BorderPane fx:id="border" prefHeight="200.0" prefWidth="200.0">
               <center>
                  <Group fx:id="arbre" BorderPane.alignment="TOP_CENTER">
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                  </Group>
               </center>
               <left>
                  <Label fx:id="trouver_label" text="Label" BorderPane.alignment="TOP_CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <BorderPane.margin>
                        <Insets />
                     </BorderPane.margin>
                     <padding>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </padding>
                  </Label>
               </left>
            </BorderPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
