<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="algo.ABRController">
   <top>
      <HBox style="-fx-background-color: #FFFFFF;">
         <children>
            <TextField fx:id="text" prefHeight="30.0" prefWidth="200.0">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <Button mnemonicParsing="false" onAction="#insert" prefHeight="30.0" text="Inserer">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#Rechercher" prefHeight="30.0" text="Rechercher">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#supprimer" text="supprimer">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <RadioButton fx:id="predecesseur" mnemonicParsing="false" text="Predecesseur">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </RadioButton>
            <RadioButton fx:id="successeur" mnemonicParsing="false" text="Successeur">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </RadioButton>
            <Button mnemonicParsing="false" onAction="#FileTXT" text="File TXT">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" BorderPane.alignment="TOP_CENTER">
         <content>
            <BorderPane fx:id="border" prefHeight="200.0" prefWidth="200.0">
               <center>
                  <AnchorPane fx:id="arbre" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
               </center>
               <top>
                  <BorderPane BorderPane.alignment="CENTER">
                     <left>
                        <Label fx:id="trouver_label" text="Label" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                           </BorderPane.margin>
                        </Label>
                     </left>
                     <center>
                        <HBox fx:id="List_Nombre" alignment="CENTER" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets left="50.0" />
                           </BorderPane.margin>
                        </HBox>
                     </center>
                  </BorderPane>
               </top>
            </BorderPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
